{
  "name": "@felipeoriani/flight-service",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --fix --ext .ts .",
    "test": "globstar -- node --import tsx --test \"tests/**/*.test.ts\"",
    "type-check": "tsc --pretty --noEmit",
